curl -H "Authorization: Token a6277616815ed5c4cf164c6ea9d2f0599fc7faf8" http://localhost:8000/api/create-lesson/ -H "Content-Type: application/json" -X POST -d "{\"course\": 1, \"lessonName\": \"Lesson 6\", \"questionsNo\": \"10\"}"

curl for starting a quiz:
curl -H "Authorization: Token a6277616815ed5c4cf164c6ea9d2f0599fc7faf8" http://localhost:8000/api/start-quiz/ -H "Content-Type: application/json" -X POST -d "{\"email\": \"pouria@pouria.com\", \"course_id\": 5520, \"lesson_name\": \"lesson 1\"}"


curl for ending a quiz:
curl -H "Authorization: Token a6277616815ed5c4cf164c6ea9d2f0599fc7faf8" http://localhost:8000/api/end-quiz/ -H "Content-Type: application/json" -X POST -d "{\"email\": \"pouria@pouria.com\", \"course_id\": 5520, \"lesson_name\": \"all\"}"


curl for answer the question:
curl -H "Authorization: Token a6277616815ed5c4cf164c6ea9d2f0599fc7faf8" http://localhost:8000/api/question-answer/ -H "Content-Type: application/json" -X POST -d "{\"email\": \"pouria@pouria.com\", \"course_id\": 5520, \"lesson_name\": \"lesson 2\", \"question_number\": 1, \"index\": \"c\" }"


